on_stop_ask', function(){\n        _survey.stop_ask('{{=it.survey.id}}');\n    });\n    $container.on('click', '._mktz_lead_send', function(){\n        _survey._send_lead();\n    });\n    $container.on('click', '._mktz_action_privacy_btn', function(){\n        _survey.slideToggle('._mktz_privacy_content');\n    });\n    $container.on('click', '._mktz_slide_prev', function(){\n        _survey.navigate_back('swipe_left_animation');\n    });\n    $container.on('click', '._mktz_slide_next', function(){\n        _survey.navigate_forward('swipe_right_animation');\n    });\n    $container.on('click', '._mktz_close', function(){\n        _survey._close();\n    });\n    $container.on('keypress', 'input,select', function(e){\n        var code = (e.keyCode ? e.keyCode : e.which);\n        var $question = mktz_$('div._mktz_question:visible');\n\n        if (code == 13) {\n            if ($question.next().is('#_mktz_end, ._mktz_end')) {\n                $question.find('._mktz_action_send').trigger('click');\n            } else {\n                $question.find('._mktz_action_next').trigger('click');\n            }\n        }\n    });\n    \n    $container.on('focus, click', 'input[type=text], textarea', function(){\n        mktz_$(this).closest('#_mktz_survey').addClass('omniKeyboardActive');\n    });\n    $container.on('click', '*', function(e){\n        var $elem = mktz_$(e.target);\n        if (\n            $elem.prop('tagName') != 'TEXTAREA' &&\n            !($elem.prop('tagName') == 'INPUT' && $elem.attr('type') == 'text')\n        ) {\n            mktz_$(this).closest('#_mktz_survey').removeClass('omniKeyboardActive');            \n        }\n    });\n})();\n<\/script>","code_widget":"\n{{##def.nav:\n    <!-- navigation -->\n    <div class=\"_mktz_navigation\">\n        <div class=\"_mktz_slide\">\n            <div class=\"_mktz_controls_start\">\n                <button class=\"_mktz_action_start _mktz_btn\">\n                    {{=it.survey.translation.START_BTN}}\n   