lection) { if (!{}.hasOwnProperty.call(reviewsCollection, key)) { continue; } slide = getCarouselSlide(reviewsCollection[key], key); $reviewsCarousel.find('.swiper-wrapper').append(slide); } } function getCarouselSlide(data, key) { return '' + '<div class=\"omni-slide swiper-slide\" data-content=\"' + data.reviewContent + '\">' + ' <div class=\"omni-header clearfix\">' + ' <div class=\"omni-stars-{MKTZ[EXP_ID]}\" data-stars=\"' + data.starsNumber + '\">'+ ' <span class=\"glyphicon glyphicon-star omni-star\"><\/span>' + ' <span class=\"glyphicon glyphicon-star omni-star\"><\/span>' + ' <span class=\"glyphicon glyphicon-star omni-star\"><\/span>' + ' <span class=\"glyphicon glyphicon-star omni-star\"><\/span>' + ' <span class=\"glyphicon glyphicon-star omni-star\"><\/span>' + ' <\/div>' + ' <div class=\"omni-reviewer-name\">' + data.reviewerName + ' <\/div>' + ' <\/div>' + ' <div class=\"omni-content\">' + data.reviewContent + '<\/div>' + '<\/div>' ; } function runCarousel(deviceClass, $reviewsCarousel) { var swiperUrl = 'https:\/\/cdnjs.cloudflare.com\/ajax\/libs\/Swiper\/4.5.0\/js\/swiper.min.js'; insertCarouselCssFile(); $.getScript(swiperUrl) .then(function() {initCarousel(deviceClass);}) .then(addListeners) .then(function() { trackCarousel(deviceClass); attachSeeMoreFunctionality($reviewsCarousel); }) ; } function insertCarouselCssFile() { var cssFile = '<link rel=\"stylesheet\" href=\"https:\/\/cdnjs.cloudflare.com\/ajax\/libs\/Swiper\/4.5.0\/css\/swiper.min.css\">'; var $target = $('body'); $target.prepend(cssFile); } function initCarousel(deviceClass) { var carousel = '.swiper-container.' + deviceClass; var initSwiper = new Swiper('.swiper-container.' + deviceClass, { effect: 'fade', fadeEffect: { crossFade: true }, autoplay: { delay: 5000, disableOnInteraction: true }, loop: true, pagination: { el: '.swiper-pagination', clickable: true }, navigation: { nextEl: '.swiper-button-next', prevEl: '.swiper-button-prev' }, observer: true, observeParents: true, on: { init: function () { $(carousel).parent().addClass('omni-swiper-ready'); } } }); initSwiper.init(); initSwiper.on('touchStart', function() { api.goal(':omniId-s-swipe'); api.goal(':omniId-s-all-carousel-interactions'); }); } function addListeners() { var $swiperContainer = $('.omni-container-{MKTZ[EXP_ID]}').find('.swiper-container'); $swiperContainer .on('mouseenter touchstart', function stopCarousel() { this.swiper.autoplay.stop(); }) .on('mouseleave', function startCarousel() { this.swiper.autoplay.start(); }) ; } function trackCarousel(deviceClass) { var $carousel = $('.omni-container-{MKTZ[EXP_ID]}').has('.' + deviceClass); api.track($carousel.find('.swiper-pagination .swiper-pagination-bullet'), { name: ':omniId-s-bullet-click' }); api.track($carousel.find('.swiper-button-next'), { name: ':omniId-s-click-next' }); api.track($carousel.find('.swiper-button-prev'), { name: ':omniId-s-click-prev' }); api.track($carousel.find('.swiper-pagination .swiper-pagination-bullet, .swiper-button-next, .swiper-button-prev'), { name: ':omniId-s-all-carousel-interactions' }); } function attachSeeMoreFunctionality($reviewsCarousel) { var $reviews = $reviewsCarousel.find('.omni-content'); $reviews.find('.omni-see-more').on('click', function(event) { api.goal(':omniId-s-see-more-click'); api.goal(':omniId-s-all-carousel-interactions'); var $self = $(event.target); var fullText = $self.closest('.omni-slide').attr('data-content'); $self.closest('.omni-content').html(fullText); \/** update the duplicated slider too *\/ $reviewsCarousel.find('.omni-slide[data-content=\"'+ fullText + '\"]').find('.omni-content').html(fullText); }); } function waitForMobile() { return _mktz.promised.dom('.new-product-container', 'guard-{MKTZ[EXP_ID]}') .catch(waitForMobile); } function getData() { return { 'https:\/\/www.elefant.ro\/apa-de-parfum-calvin-klein-euphoria-100-ml-pentru-femei_07e29b2d-9cd9-491e-b46f-9fcc86605fff': { reviews: { 'review1' : { 'starsNumber' : 5, 'reviewerName' : 'Alina', 'revie