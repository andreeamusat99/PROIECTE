function hashify(message){var hash=0,i,chr,len;if(message.length===0)return hash;for(i=0,len=message.length;i<len;i++){chr=message.charCodeAt(i);hash=((hash<<5)-hash)+chr;hash|=0;}
if(hash<0){hash=hash*(-1);}
return hash;}
